{
  "version": 2,
  "builds": [
    {
      "src": "src/serverless.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["assets/**"],
        "maxDuration": 60,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": { "Location": "/api" }
    },
    {
      "src": "/(.*)",
      "dest": "src/serverless.ts"
    }
  ]
}
